                        <option value="16"> 16 </option>
                        <option value="17"> 17 </option>
                        <option value="18"> 18 </option>
                        <option value="19"> 19 </option>
                        <option value="20"> 20 </option>
                    </select>
				</center>
				<h3 class="mb-0 mt-3"> تعداد داخلی مورد نظر را انتخاب کنید :</h3>
				<center>
                    <select asp-for="InternalNum" class="form-control" style="width: 100%;" required disabled>
